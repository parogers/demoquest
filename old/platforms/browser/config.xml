<?xml version='1.0' encoding='utf-8'?>
<widget id="com.github.parogers.demoquest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DemoQuest</name>
    <description>
      A simple point and click style adventure game.
    </description>
    <author email="peter.rogers@gmail.com" href="https://github.com/parogers/demoquest">
        Peter Rogers
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
